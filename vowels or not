#include<iostream>
using namespace std;
int main(){
char c;
cout<<"enter the character and check the vowel";
cin>>c;
if(c=='a'|| c=='A' || c=='e'|| c=='E'|| c=='i'|| c=='I'|| c=='o'|| c=='O'|| c=='u'|| c=='U'){
    cout<<"this is vowel"<<endl;
}
else{
        cout<<"this is not vowel"<<endl;
    }
}
